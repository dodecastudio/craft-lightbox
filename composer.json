{
  "name": "dodecastudio/craft-lightbox",
  "description": "A simple, accessible and customisable lightbox gallery that works easily with Craft CMS.",
  "type": "craft-plugin",
  "version": "1.0.0-beta.1",
  "license": "MIT",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "lightbox",
    "gallery",
    "assets",
    "images"
  ],
  "support": {
    "source": "https://github.com/dodecastudio/craft-lightbox",
    "docs": "https://github.com/dodecastudio/craft-lightbox/blob/main/README.md",
    "issues": "https://github.com/dodecastudio/craft-lightbox/issues"
  },
  "authors": [
    {
      "name": "Dodeca Studio",
      "homepage": "https://dodeca.studio"
    }
  ],
  "require": {
    "craftcms/cms": "^4.0.0-beta"
  },
  "autoload": {
    "psr-4": {
      "dodecastudio\\lightbox\\": "src/"
    }
  },
  "extra": {
    "name": "Lightbox",
    "handle": "lightbox",
    "developer": "Dodeca Studio",
    "developerUrl": "https://dodeca.studio",
    "documentationUrl": "https://github.com/dodecastudio/craft-lightbox/blob/main/README.md",
    "changelogUrl": "https://github.com/dodecastudio/craft-lightbox/blob/main/CHANGELOG.md",
    "class": "dodecastudio\\lightbox\\lightbox"
  }
}
