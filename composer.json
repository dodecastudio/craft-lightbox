{
  "name": "dodecastudio/craft-lightbox",
  "description": "A simple, responsive, accessible and customisable lightbox gallery that integrates seamlessly with Craft CMS.",
  "type": "craft-plugin",
  "version": "2.0.1",
  "license": "proprietary",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "lightbox",
    "gallery",
    "assets",
    "images"
  ],
  "support": {
    "source": "https://github.com/dodecastudio/craft-lightbox",
    "docs": "https://github.com/dodecastudio/craft-lightbox/wiki",
    "issues": "https://github.com/dodecastudio/craft-lightbox/issues"
  },
  "authors": [
    {
      "name": "Dodeca Studio",
      "homepage": "https://dodeca.studio"
    }
  ],
  "require": {
    "php": ">=7.4",
    "craftcms/cms": "^3.0|^4.0|^5.0"
  },
  "autoload": {
    "psr-4": {
      "dodecastudio\\lightbox\\": "src/"
    }
  },
  "extra": {
    "name": "Lightbox",
    "handle": "lightbox",
    "developer": "Dodeca Studio",
    "developerUrl": "https://dodeca.studio",
    "documentationUrl": "https://github.com/dodecastudio/craft-lightbox/blob/main/README.md",
    "changelogUrl": "https://github.com/dodecastudio/craft-lightbox/blob/main/CHANGELOG.md",
    "class": "dodecastudio\\lightbox\\Lightbox",
    "components": {
      "services": "dodecastudio\\lightbox\\services\\LightboxService"
    }
  }
}
